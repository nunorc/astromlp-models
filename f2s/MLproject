
name: f2s

entry_points:
  main:
    parameters:
      epochs: { type: float, default: 100 }
      batch_size: { type: float, default: 64 }
      optimizer: { type: string, default: 'adam' }
      ds: { type: string, default: '../../sdss-gs' }
    command: "python train.py {epochs} {batch_size} {optimizer} {ds}"

